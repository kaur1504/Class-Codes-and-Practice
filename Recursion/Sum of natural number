public class sum_of_natural_no {
    public static void main(String[] args) {
        int n=5;
        System.out.println(sum(n));
    }
    public static int sum(int n){
        System.out.print(n);
        if(n==1){
            return 1;
        }
        int ans=n+sum(n-1);
        return ans;
    }
}
